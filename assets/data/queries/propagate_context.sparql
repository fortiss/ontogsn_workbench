PREFIX gsn:   <https://w3id.org/OntoGSN/ontology#>
PREFIX schema:<https://schema.org/>
PREFIX case: <https://w3id.org/OntoGSN/cases/ACT-FAST-robust-llm#>

SELECT DISTINCT ?nodeIRI ?node ?fromTop ?context
WHERE {
  VALUES ?ctx { {{CTX_IRI}} }
  ?top gsn:inContextOf ?ctx ;
       schema:identifier ?fromTop .
  ?ctx a gsn:Context ; 
       schema:identifier ?context .
  ?top (gsn:supportedBy)* ?nodeIRI .
  ?nodeIRI schema:identifier ?node .
}
ORDER BY ?fromTop ?node