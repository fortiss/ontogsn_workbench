@prefix schema: <https://schema.org/> .
@prefix ex:     <https://example.org/car-demo#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .

ex:car1 a schema:Car ;
  schema:name "OntoGSN Demo Hatchback" ;
  schema:alternateName "Zastava 101-inspired OntoGSN demo" ;
  schema:description "Small late-1970s style Yugoslav hatchback used as an OntoGSN demo car in a knowledge-augmented assurance case visualization." ;
  schema:sameAs <https://en.wikipedia.org/wiki/Zastava_101> ;
  schema:vehicleIdentificationNumber "0N7065N" ;
  schema:acrissCode "ECMN" ;
  schema:bodyType "3-door hatchback" ;
  schema:vehicleConfiguration "3dr hatchback, 1.1L, FWD, 4M" ;

  schema:brand [
    a schema:Brand ;
    schema:name "Zastava"
  ] ;

  schema:manufacturer [
    a schema:Organization ;
    schema:name "Zastava Automobili" ;
    schema:countryOfOrigin [
      a schema:Country ;
      schema:name "Yugoslavia"
    ]
  ] ;

  schema:model "101 1.1" ;
  schema:vehicleModelDate "1971-10-15"^^xsd:date ;
  schema:modelDate        "1971-10-15"^^xsd:date ;

  schema:productionDate             "1983-06-01"^^xsd:date ;
  schema:dateVehicleFirstRegistered "1983-07-01"^^xsd:date ;
  schema:purchaseDate               "1989-05-20"^^xsd:date ;
  schema:numberOfPreviousOwners     3 ;

  schema:numberOfDoors 3 ;
  schema:numberOfAxles         2 ;
  schema:numberOfForwardGears  4 ;
  schema:vehicleTransmission   "4-speed manual" ;
  schema:driveWheelConfiguration schema:FrontWheelDriveConfiguration ;
  schema:steeringPosition        schema:LeftHandDriving ;

  schema:vehicleSeatingCapacity 5 ;
  schema:seatingCapacity        5 ;
  schema:vehicleInteriorColor   "brown cloth" ;
  schema:vehicleInteriorType    "cloth seats, plastic dashboard" ;

  schema:numberOfAirbags 0 ;
  schema:knownVehicleDamages
    "Light rust on rear wheel arches and worn driver seat upholstery." ;

  schema:color "light olive green" ;
  schema:category "classic compact car" ;

  # Dimensions (you can map these directly to BODY_* etc.)
  schema:width  [ a schema:QuantitativeValue ;
                  schema:value "2.0"^^xsd:float ;
                  schema:unitCode "MTR" ] ;
  schema:height [ a schema:QuantitativeValue ;
                  schema:value "1.55"^^xsd:float ;
                  schema:unitCode "MTR" ] ;
  schema:depth  [ a schema:QuantitativeValue ;
                  schema:value "4.54"^^xsd:float ;
                  schema:unitCode "MTR" ] ;
  schema:wheelbase [ a schema:QuantitativeValue ;
                     schema:value    "2.44"^^xsd:float ;
                     schema:unitCode "MTR" ] ;

  schema:weightTotal [
    a schema:QuantitativeValue ;
    schema:value    "1220"^^xsd:float ;    # 820 kg curb + ~400 kg payload (example)
    schema:unitCode "KGM"
  ] ;
  schema:payload [
    a schema:QuantitativeValue ;
    schema:value    "400"^^xsd:float ;
    schema:unitCode "KGM"
  ] ;
  schema:roofLoad [
    a schema:QuantitativeValue ;
    schema:value    "50"^^xsd:float ;      # typical small-car roof rack limit (example)
    schema:unitCode "KGM"
  ] ;
  schema:cargoVolume [
    a schema:QuantitativeValue ;
    schema:value    "320"^^xsd:float ;     # approx. rear hatch volume in litres
    schema:unitCode "LTR"
  ] ;

  schema:accelerationTime [
    a schema:QuantitativeValue ;
    schema:value    "16"^^xsd:float ;      # 0–100 km/h in ~16 s (varies by source):contentReference[oaicite:4]{index=4}
    schema:unitCode "SEC" ;
    schema:name     "0–100 km/h"
  ] ;
  schema:speed [
    a schema:QuantitativeValue ;
    schema:maxValue "135"^^xsd:float ;     # km/h top speed
    schema:unitCode "KMH"
  ] ;
  schema:fuelCapacity [
    a schema:QuantitativeValue ;
    schema:value    "39"^^xsd:float ;      # 39 L tank
    schema:unitCode "LTR"
  ] ;
  schema:fuelConsumption [
    a schema:QuantitativeValue ;
    schema:value    "7.0"^^xsd:float ;     # combined L/100 km
    schema:unitText "L/100 km (combined)"
  ] ;
  schema:fuelEfficiency [
    a schema:QuantitativeValue ;
    schema:value    "14.3"^^xsd:float ;    # ~100 / 7
    schema:unitText "km/L (combined)"
  ] ;
  schema:mileageFromOdometer [
    a schema:QuantitativeValue ;
    schema:value    "186000"^^xsd:float ;  # example mileage
    schema:unitCode "KMT"
  ] ;
  schema:emissionsCO2 "165"^^xsd:float ;   # rough estimate from ~7 L/100 km petrol

  # Engine (EngineSpecification)
  schema:fuelType "petrol" ;
  schema:vehicleEngine ex:engine1 ;

  # Parts (your clickable meshes)
  ex:hasPart ex:frontLeftWheel, ex:frontRightWheel,
             ex:rearLeftWheel,  ex:rearRightWheel,
             ex:frontLicensePlate,
             ex:leftWindow, ex:rightWindow,
             ex:leftDoor, ex:rightDoor,
             ex:frontBumper, ex:Cabin .

ex:car1
  ex:bodyLength "4.54"^^xsd:float ;
  ex:bodyHeight "0.75"^^xsd:float ;
  ex:bodyDepth  "2.0"^^xsd:float ;
  ex:bodyCenterY "0.9"^^xsd:float ;
  ex:bumperThicknessX "0.05"^^xsd:float ;
  ex:bumperHeightY    "0.10"^^xsd:float ;
  ex:bumperDepthZ     "1.80"^^xsd:float ;
  ex:doorBottomY   "0.525"^^xsd:float ;
  ex:doorTopY      "1.28"^^xsd:float ;
  ex:doorFrontX    "0.9"^^xsd:float ;
  ex:doorBackX     "-0.4"^^xsd:float ;
  ex:doorDepthZ    "0.12"^^xsd:float ;
  ex:doorDepthOffset "0.95"^^xsd:float ;
  ex:doorBottomY  "0.525"^^xsd:float ;
  ex:doorTopY     "1.28"^^xsd:float ;
  ex:doorFrontX   "0.90"^^xsd:float ;
  ex:doorBackX   "-0.40"^^xsd:float ;
  ex:doorDepthZ   "0.12"^^xsd:float ;
  ex:doorZOffset  "0.95"^^xsd:float ;
  ex:cabinWidthX   "2.15"^^xsd:float ;
  ex:cabinHeightY  "0.80"^^xsd:float ;
  ex:cabinDepthZ   "1.70"^^xsd:float ;
  ex:cabinCenterX "-0.50"^^xsd:float ;
  ex:cabinCenterY  "1.55"^^xsd:float .

ex:engine1 a schema:EngineSpecification ;
  schema:engineDisplacement [
    a schema:QuantitativeValue ;
    schema:value    "1116"^^xsd:float ;    # 1.116 L
    schema:unitCode "CMQ"
  ] ;
  schema:enginePower [
    a schema:QuantitativeValue ;
    schema:value    "41"^^xsd:float ;      # kW ≈ 56 hp
    schema:unitCode "KWT"
  ] ;
  schema:torque [
    a schema:QuantitativeValue ;
    schema:value    "79"^^xsd:float ;      # Nm (approx from sources)
    schema:unitCode "NU"
  ] ;
  schema:engineType "Inline 4, overhead cam" ;
  schema:fuelType   "petrol" .

ex:StandardWheel 
  rdfs:subClassOf schema:Product ;
  schema:name     "Standard Wheel" ;
  ex:type         "Cylinder" ;
  ex:radius       "0.4"  ^^xsd:float ;
  ex:width        "0.5"  ^^xsd:float ;
  ex:segments     "24"   ^^xsd:float ;
  ex:y            "0.5"  ^^xsd:float ;
  ex:offsetX      "1.4"  ^^xsd:float ;
  ex:offsetZ      "0.85" ^^xsd:float .

ex:frontLeftWheel a ex:StandardWheel ;
  schema:name "Front-left wheel" ;
  schema:isPartOf ex:car1 ;
  ex:positionOnVehicle "front-left" .

ex:frontRightWheel a ex:StandardWheel ;
  schema:name "Front-right wheel" ;
  schema:isPartOf ex:car1 ;
  ex:positionOnVehicle "front-right" .

ex:rearLeftWheel a ex:StandardWheel ;
  schema:name "Rear-left wheel" ;
  schema:isPartOf ex:car1 ;
  ex:positionOnVehicle "rear-left" .

ex:rearRightWheel a ex:StandardWheel ;
  schema:name "Rear-right wheel" ;
  schema:isPartOf ex:car1 ;
  ex:positionOnVehicle "rear-right" .

ex:frontLicensePlate a schema:Product ;
  schema:name "Front license plate" ;
  schema:isPartOf ex:car1 .

ex:leftWindow a schema:Product ;
  schema:name "Left window" ;
  schema:isPartOf ex:car1 .

ex:rightWindow a schema:Product ;
  schema:name "Right window" ;
  schema:isPartOf ex:car1 .

ex:leftDoor a schema:Product ;
  schema:name "Left door" ;
  schema:isPartOf ex:car1 .

ex:rightDoor a schema:Product ;
  schema:name "Right door" ;
  schema:isPartOf ex:car1 .

ex:frontBumper a schema:Product ;
  schema:name "Front bumper" ;
  schema:isPartOf ex:car1 .

ex:Cabin a schema:Product ;
  schema:name "Cabin" ;
  schema:isPartOf ex:car1 .


# …and so on for lights, mirrors, doors, bumper, license plate, etc.
