PREFIX gsn:  <https://w3id.org/OntoGSN/ontology#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX case: <https://w3id.org/OntoGSN/cases/ACT-FAST-robust-llm#>
PREFIX ex:   <https://example.org/car-demo#>

SELECT DISTINCT ?s ?p ?o ?type
WHERE {
    BIND(<{{MODULE_IRI}}> AS ?mod)
    ?mod gsn:contains ?s .
    ?s ?p ?o .
    OPTIONAL { ?s a ?typeS . }
    OPTIONAL { ?o a ?typeO . }
    FILTER(?p IN (gsn:supportedBy, gsn:inContextOf, gsn:challenges))
}
ORDER BY ?s ?p ?o ?type
