asce:contains rdf:type owl:ObjectProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( 
                                   asce:ASCENetwork
                                   asce:View
                                   asce:ExportPath
                                   )
                          ] ;
              rdfs:range owl:Thing ;
              rdfs:label "contains"@en .

